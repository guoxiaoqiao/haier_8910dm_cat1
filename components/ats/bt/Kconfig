menu "at_bt"

config AT_SPBTTEST_SUPPORT
    bool "AT_SPBTTEST_SUPPORT enabled"
    default n
    help
        When enabled, CONFIG_AT_SPBTTEST_SUPPORT will be defined

config AT_SPBLETEST_SUPPORT
    bool "AT_SPBLETEST_SUPPORT enabled"
    default n
    help
        When enabled, CONFIG_AT_SPBLETEST_SUPPORT will be defined

config AT_SPBLE_SUPPORT
    bool "AT_SPBLE_SUPPORT enabled"
    default n
    select BLUEU_BT_ENABLE
    help
        When enabled, CONFIG_AT_SPBLE_SUPPORT will be defined

config AT_BT_CLASSIC_SUPPORT
    bool "AT_BT_CLASSIC_SUPPORT enabled"
    default n
    select BLUEU_BT_ENABLE
    help
        When enabled, CONFIG_AT_BT_CLASSIC_SUPPORT will be defined

config AT_BTCOMM_SUPPORT
    bool "AT_BTCOMM_SUPPORT enabled"
    default n
    select BLUEU_BT_ENABLE
    help
        When enabled, CONFIG_AT_BTCOMM_SUPPORT will be defined

endmenu
